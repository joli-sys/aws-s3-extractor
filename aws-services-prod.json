{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "S3Extractor": {
      "Type": "AWS::IAM::User",
      "Properties": {
        "Path": "/",
        "Policies": [
          {
            "PolicyName": "stsAssumeRole",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": {
                "Effect": "Allow",
                "Action": "sts:AssumeRole",
                "Resource": "arn:aws:iam::*:role/keboola-s3-extractor"
              }
            }
          }
        ]
      }
    }
  }
}
